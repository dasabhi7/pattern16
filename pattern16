#include<stdio.h> 
int main()
{
   int n;
   scanf("%d",&n);
   int k=1;
   while(k<=n){
      int space=1;
      while(space<k){
          printf(" ");
          space++;
      }
      int a=k;
      while(a<=n){
          printf("%d",a);
          a++;
      }
       printf("\n");
       k++;
   }
}
